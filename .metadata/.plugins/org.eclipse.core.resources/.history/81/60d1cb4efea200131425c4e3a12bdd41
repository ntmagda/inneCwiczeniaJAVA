
public class Lista<T> {
	Node<Integer> pierwszy = new Node<Integer>(5);
	Node<T> top = new Node<T>(); // ostatni element listy
	
	public void push(Node<T> item){ // wstawia element na koniec listy
		if(top.item!=null)
		{
			top.next = item;
			item.next = null;
		}
		else {
			top.item = item.item;
			top.next = null;
		}
	}
	
	public Node<T> pop() // zwraca ostatni element listy i przypisuje do topa 
	{
		Node<T> result = top;
		if(result.next!= null)
		{
			result = result.next;
		}
		top = result;
		return result;
	}

}
