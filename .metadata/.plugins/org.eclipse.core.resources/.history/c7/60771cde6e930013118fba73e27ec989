
public class TablicaDouble 
{

	public static void main(String[] args)
	{
		
	}
	
	double[] tworzTablica(int rozmiar, double poczatek, double koniec)
	{
		double[] tablica= new double[rozmiar];
		double iterator;
		iterator = (koniec - poczatek)/rozmiar;
		for ( int i =0; i <rozmiar; i ++)
		{
			tablica[i] = poczatek+i;
		}
		return tablica;
	}
	
}
