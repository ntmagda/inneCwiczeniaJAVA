
public class Node<T> {
	
	T item;
	Node<T> next;
	Node(){
		item = null;
		next = null;
	}
	Node(T item, Node<T> next)
	{
		this.item = item;
		this.next = next;
	}
	boolean ifEnd()
	{
		return ( this.item== null && this.next == null); 
	}
	Node<T> top = new Node<T>();
	
	public void push(Node<T> item){
		if(top.item!=null)
		{
			top.next = item;
			item.next = null;
		}
		else {
			top.item = item.item;
			top.next = null;
		}
	}
	
	public void top

}
